{
   "name": "<%_ if (isOpenSource) { _%>@silvermine/<%_ } _%><%= projectName %>",
   "version": "0.1.0",
   "description": "",
   <%_ if (isLibrary) { _%>
   "main": "./dist/commonjs/index",
   "types": "./dist/types/index.d.ts",
   "module": "./dist/esm/index",
   <%_ } _%>
   "scripts": {
      "test": "TS_NODE_PROJECT='tests/tsconfig.json' TS_NODE_FILES=true nyc mocha --opts ./.mocha.opts"
   },
   "author": "<%= author %>",
   "license": "<%= isOpenSource ? 'MIT' : 'PROPRIETARY' %>",
   <%_ if (isOpenSource) { _%>
   "repository": {
      "type": "git",
      "url": "git+https://github.com/silvermine/<%= projectName %>.git"
   },
   "bugs": {
      "url": "https://github.com/silvermine/<%= projectName %>/issues"
   },
   "homepage": "https://github.com/silvermine/<%= projectName %>#readme",
   <%_ } _%>
   "engines": {
      "node": ">=8.10.0",
      "npm": "6.4.1"
   },
   "devDependencies": {
      "@silvermine/chai-strictly-equal": "1.1.0",
      "@types/chai": "4.1.7",
      "@types/mocha": "5.2.5",
      "@types/node": "8.10.36",
      "@types/sinon": "5.0.5",
      "coveralls": "3.0.2",
      "chai": "4.2.0",
      "grunt": "1.0.3",
      "grunt-cli": "1.3.1",
      <%_ if (isLibrary) { _%>
      "grunt-concurrent": "2.3.1",
      <%_ } _%>
      "grunt-contrib-clean": "2.0.0",
      "grunt-contrib-watch": "1.1.0",
      "grunt-eslint": "21.0.0",
      "grunt-exec": "3.0.0",
      <%_ if (isBrowser) { _%>
      "jsdom": "13.0.0",
      "jsdom-global": "3.0.2",
      <%_ } _%>
      "mocha": "5.2.0",
      "nyc": "13.3.0",
      "sinon": "5.1.1",
      "source-map-support": "0.5.9",
<%_ if (!isLibrary) { _%>
      "tslib": "1.9.3",
<%_ } _%>
      "ts-node": "7.0.1",
      <%_ if (isBrowser) { _%>
      "ts-loader": "5.3.0",
      <%_ } _%>
      "typescript": "3.2.2"<% if (isBrowser) { %>,
      "webpack": "4.28.3",
      "webpack-cli": "3.2.0"
      <%_ } _%>
   },
   "dependencies": {
<%_ if (isLibrary) { _%>
      "tslib": "1.9.3"
<%_ } _%>
   }
 }
